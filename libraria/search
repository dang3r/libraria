  {{#liking_ifContentHasLikes like_count}}
    <h4 class="like_breakdown_heading">
        {{#liking_if_single_user_liked like_count}}
          {{liking_liked_users_count like_count}} likes this
        {{else}}
          {{liking_liked_users_count like_count}} like this
        {{/liking_if_single_user_liked}}
