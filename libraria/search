    <span class="like_usernames">
      <i aria-hidden="true" class=" icon-heart"></i>
      {{#if liked}}
        You{{#liking_unlessContentOnlyHasOneLike like_count}},{{/liking_unlessContentOnlyHasOneLike}}
      {{/if}}
      {{#each liked_users.users}}
        {{#if is_active}}
          <a class="like_username" href="/user_profile/{{id}}">{{username}}</a>{{#unless @last}},{{/unless}}
        {{else}}
          <span class="like_username" title="Inactive User">{{username}}</span>{{#unless @last}},{{/unless}}
        {{/if}}
      {{/each}}
    </span>

    {{#if liked_users.see_more}}
      <div class="like_usernames_see_more_wrap">
        <a href="{{url}}" data-js="like_breakdown_see_more">See more<i aria-hidden="true" class=" icon-down-dir"></i></a>
        <i aria-hidden="true" class="hidden icon-spinner" data-js="liked_users_spinner"></i>
      </div>
    {{/if}}

  {{else}}
    <h4 class="like_breakdown_heading">No one has liked this yet. Be the first!</h4>
  {{/liking_ifContentHasLikes}}
</script>
